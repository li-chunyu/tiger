a.out: main.o prog1.o slp.o util.o ex.o
	cc -g main.o prog1.o slp.o util.o ex.o

main.o: main.c slp.h util.h
	cc -g -c main.c

prog1.o: prog1.c slp.h util.h
	cc -g -c prog1.c

slp.o: slp.c slp.h util.h
	cc -g -c slp.c

util.o: util.c util.h
	cc -g -c util.c
	
ex.o: ex.c ex.h
	cc -g -c ex.c

clean: 
	rm -f a.out *.o
